  "license": "MIT",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "serve": "nodemon",
    "start:dev": "rimraf dist && webpack --config webpack/webpack.dev.js && node dist/server.js",
    "start:prod": "NODE_ENV=production rimraf dist && webpack --config webpack/webpack.prod.js && node dist/server.js",
    "build": "NODE_ENV=production rimraf dist && webpack --config webpack/webpack.prod.js",
    "clean": "rimraf dist",
    "lint": "eslint src/server.ts",
    "lint:fix": "eslint --fix src/server.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.3.1",
    "@koa/cors": "^3.0.0",
    "@koa/multer": "^2.0.2",
    "@koa/router": "^8.0.8",
    "@typegoose/typegoose": "^6.3.2",
    "async": "^3.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "0.10.1",
    "dotenv": "^8.2.0",
    "grant-koa": "^4.7.0",
    "ioredis": "^4.14.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.1",
    "koa-router": "^8.0.0-alpha.1",
    "mailgun-js": "^0.22.0",
    "mongoose": "^5.8.11",
    "multer": "^1.4.2",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.0-alpha.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/ioredis": "^4.14.7",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-logger": "^3.1.1",
    "@types/mailgun-js": "^0.22.4",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.7.0",
    "@types/node-fetch": "^2.5.4",
    "@types/pug": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
